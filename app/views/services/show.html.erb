<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <div class="banner-show" style="background: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('<%= image_path 'background.jpg' %>'); height: 100px; background-size: cover; background-position: center center; margin-top: 7px; ">
      </div>
      <div class="info text-center">
        <%= image_tag @service.photo_url, class: "avatar-square" %>
        <h3>Name: <%= @service.title %></h3>
        <h3>Location: <%= @service.city %></h3>
      </div>
      <div class="service text-center">
        <h2>Education:</h2>
        <p><%= @service.education %></p>
        <h2>Experience:</h2>
        <p><%= @service.experience %> years</p>
      </div>
      <div class="experience text-center">
        <h2>Rate></h2>
        <p><%= @service.rate %></p>
      </div>
    </div>
  </div>
</div>







