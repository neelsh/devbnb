
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <%= simple_form_for(@user) do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :city %>
      <%= f.input :photo, as: :attachinary %>
      <%= f.submit :update %>
    <% end %>
    </div>
  </div>
</div>
